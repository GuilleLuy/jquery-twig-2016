<!doctype html>
<html>
{% block head %}
<head>
<title>{% block titulo %}{% endblock %}</title>
<meta charset="utf8" />
<meta name="description" content="{% block description %}{% endblock %}"/>
<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"/>
{# <meta name='viewport' content='width=device-width, initial-scale=1' /> #}
<meta name="robots" content="index,follow"/>
<link rel="apple-touch-icon" sizes="180x180" href="/media/img/apple-icon.png">
<link rel="icon" type="image/png" sizes="192x192"  href="/media/img/android-icon.png">
<link rel='icon' href='/media/img/favicon.ico'>
<link href="/css/styles.css" rel="stylesheet">
<link href='{{ AUTOR_WEB }}' rel='canonical'> 
<meta name='google-site-verification' content='LZuShXnWkQsSEozBme_KaJZfHCMhqI2h3FHYQL66g3A' />
<script>document.documentElement.className = document.documentElement.className.replace(/\bno-js\b/,'js');</script>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
  ga('create', 'UA-846602-1', 'auto');
  ga('send', 'pageview');
</script>{% endblock %}
</head>
<body{% if id_body is defined %} id="{{ id_body|raw }}"{% endif %}>
{% block body %}
<div id="main-preloader" class="main-preloader"><div class="main-preloader-inner center"><h2 class="preloader-percentage center"><span class="preloader-percentage-text">0</span><span class="percentage">%</span></h2><div class="preloader-bar-outer"><div class="preloader-bar"></div></div></div></div>
{% block content %}{% endblock %}

<footer id="footer">
{% block footer_nav %}{% endblock %}
  <p>&copy; {{ 'now'|date('Y') }} Guillermo Luy / {{ tel }}: {{  CLIENTE_TEL_PREFIJO ~ '-' ~ CLIENTE_TEL1 }} / E-mail: <a href="mailto:{{ cliente_mail_2|raw }}">{{ cliente_mail_2|raw }}</a></p>
  <a href="tel:{{ telefono }}" id="btn_cel" title="{{ contact_call }}"><span>{{ contact_call }}</span></a>
</footer>
{% endblock %}
{% block script %}
{# <script src="/js/jquery-3.3.1.min.js"></script> <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>#}
<script src='/js/jquery.js'></script>
<script src='/js/functions.js'></script>
{% endblock %}
</body>
</html>